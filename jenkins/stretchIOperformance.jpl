def config = ['name':"stretchIOperformance",
              'arch_list':["amd64"],
              'debian_release':"stretch",
              'extra_packages':"bc gawk g++ gcc fio libaio-dev sysstat",
              'script':"scripts/stretchIOperformance.sh"]

r.buildImage(config)
